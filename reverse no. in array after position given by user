#include <stdio.h>
int main()
{
int i,n,k,a[50],j,temp;
printf("enter the value of n:");
scanf("%d",&n);
printf("enter value in array:");
for(i=0;i<n;i++)
{
    scanf("%d",&a[i]);
}
printf("enter your position:");
scanf("%d",&k);
for(i=0;i<k;i++)
{
     printf("%d",a[i]);
    
}
for(j=n-1;j>=k;j--)
{
   printf("%d",a[j]);
}
return 0;
}
